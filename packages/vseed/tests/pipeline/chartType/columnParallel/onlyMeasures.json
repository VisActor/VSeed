{
  "chartType": "columnParallel",
  "measures": [
    { "id": "sales", "alias": "销售额", "autoFormat": true },
    {
      "id": "profit",
      "alias": "利润",
      "autoFormat": false,
      "format": {
        "type": "number",
        "ratio": 1000,
        "thousandSeparator": true,
        "decimalPlaces": 0,
        "symbol": "千",
        "prefix": "利润",
        "suffix": "$"
      }
    }
  ],
  "yAxis": {
    "labelAutoHide": true,
    "labelAutoLimit": true,
    "labelAutoRotate": true,
    "labelAutoHideGap": 5,
    "labelAutoLimitLength": 80,
    "labelAutoRotateAngleRange": [0, -45, -90],
    "line": {
      "visible": true,
      "lineColor": "#606773",
      "lineWidth": 2
    },
    "label": {
      "visible": true,
      "labelColor": "#606773",
      "labelFontSize": 12,
      "labelFontWeight": 500,
      "labelAngle": 0
    },
    "grid": {
      "visible": true,
      "gridColor": "#606773",
      "gridWidth": 2
    },
    "tick": {
      "visible": true,
      "tickColor": "#606773",
      "tickInside": true,
      "tickSize": 10
    },
    "title": {
      "visible": true,
      "titleText": "日期",
      "titleColor": "#606773",
      "titleFontSize": 12,
      "titleFontWeight": 500
    },
    "autoFormat": false
  },
  "dataset": [
    { "profit": 10000, "sales": 200000 },
    { "profit": 20000, "sales": 400000 },
    { "profit": 30000, "sales": 600000 },
    { "profit": 40000, "sales": 800000 },
    { "profit": 50000, "sales": 1000000 }
  ]
}
