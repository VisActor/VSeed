    "@eslint/js": "9.39.0",
    "@rslib/core": "0.18.3",
    "@rstest/core": "0.7.0",
    "@types/node": "24.10.1",
    "eslint": "9.39.0",
    "globals": "16.4.0",
    "prettier": "^3.6.2",
    "typescript": "5.9.3",
    "typescript-eslint": "8.46.2"
