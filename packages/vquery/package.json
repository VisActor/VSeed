{
  "name": "@visactor/vquery",
  "version": "0.2.3",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rslib build",
    "dev": "rslib build --watch --no-clean",
    "lint": "eslint .",
    "test": "rstest",
    "test:update": "rstest --update",
    "test:coverage": "rstest --coverage && open ./coverage/index.html",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@duckdb/duckdb-wasm": "1.30.0",
    "kysely": "^0.27.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@rslib/core": "0.18.1",
    "@rstest/core": "0.6.5",
    "@types/node": "^22.18.10",
    "@rstest/coverage-istanbul": "0.0.5",
    "eslint": "^9.35.0",
    "globals": "^16.3.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.43.0"
  }
}
