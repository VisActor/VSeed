{
  "chartType": "area",
  "dataset": [
    { "date": "2019", "profit": 10, "sales": 20, "count": 50 },
    { "date": "2020", "profit": 30, "sales": 60, "count": 50 },
    { "date": "2021", "profit": 30, "sales": 60, "count": 50 },
    { "date": "2022", "profit": 50, "sales": 100, "count": 50 },
    { "date": "2023", "profit": 40, "sales": 80, "count": 50 }
  ],
  "annotationPoint": [
    {
      "selector": [
        {
          "field": "sales",
          "operator": "<=",
          "value": 80
        }
      ],
      "text": "销售额<=80",
      "textAlign": "right",
      "offsetX": -15
    },
    {
      "selector": [
        {
          "field": "date",
          "operator": "in",
          "value": "2023"
        }
      ],
      "text": "2023 年",
      "textAlign": "left",
      "offsetX": 0
    }
  ],
  "dimensions": [{ "id": "date", "alias": "日期", "location": "dimension" }],
  "measures": [
    { "id": "profit", "alias": "利润" },
    { "id": "sales", "alias": "销售额" },
    { "id": "count", "alias": "数量" }
  ]
}
