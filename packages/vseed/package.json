{
  "name": "@visactor/vseed",
  "version": "0.2.3",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs"
    }
  },
  "main": "./dist/cjs/index.cjs",
  "types": "./dist/esm/index.d.ts",
  "unpkg": "./dist/umd/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "rslib build --watch --no-clean",
    "build": "rslib build",
    "dev:rsdoctor": "RSDOCTOR=true rslib dev",
    "build:rsdoctor": "RSDOCTOR=true rslib build",
    "format": "prettier --write .",
    "lint": "eslint . --cache",
    "type-check": "tsc --noEmit",
    "rstest": "rstest",
    "rstest:update": "rstest --update",
    "rstest:coverage": "rstest --update --coverage && open coverage/index.html",
    "test": "vitest run",
    "test:unit": "vitest run tests/unit",
    "test:unit:coverage": "vitest run tests/unit --coverage && open coverage/index.html",
    "test:integration": "vitest run tests/integration",
    "test:integration:update": "vitest run tests/integration --update",
    "test:integration:coverage": "vitest run tests/integration --coverage && open coverage/index.html",
    "test:update": "vitest run --update",
    "test:coverage": "vitest run  --update --coverage && open coverage/index.html"
  },
  "dependencies": {
    "remeda": "2.28.0",
    "zod": "4.0.14",
    "@visactor/vdataset": "~1.0.21",
    "@visactor/vutils": "~1.0.21"
  },
  "devDependencies": {
    "@eslint/js": "9.30.0",
    "@rsdoctor/rspack-plugin": "1.2.3",
    "@rslib/core": "0.18.1",
    "@rstest/core": "0.5.2",
    "@rstest/coverage-istanbul": "0.0.3",
    "@types/node": "22.16.5",
    "@typescript-eslint/eslint-plugin": "8.38.0",
    "@visactor/vchart": "2.0.11-alpha.5",
    "@visactor/vtable": "1.22.7-alpha.5",
    "@vitest/coverage-istanbul": "3.2.4",
    "@vitest/coverage-v8": "3.2.4",
    "eslint": "9.30.0",
    "globals": "16.2.0",
    "jsdom": "26.1.0",
    "rstest-canvas-mock": "0.0.5",
    "typescript": "5.8.3",
    "typescript-eslint": "8.35.1",
    "vi-canvas-mock": "^1.0.0",
    "vitest": "3.2.4"
  },
  "sideEffects": false
}
