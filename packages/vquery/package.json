{
  "name": "@visactor/vquery",
  "version": "0.2.3",
  "type": "module",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/cjs/node.d.ts",
        "default": "./dist/cjs/node.cjs"
      },
      "import": {
        "types": "./dist/esm/browser.d.js",
        "default": "./dist/esm/browser.js"
      }
    }
  },
  "main": "./dist/cjs/index.cjs",
  "types": "./dist/esm/web.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rslib build",
    "dev": "rslib build --watch --no-clean",
    "lint": "eslint .",
    "test": "rstest",
    "test:update": "rstest --update",
    "test:coverage": "rstest --coverage && open ./coverage/index.html",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@duckdb/duckdb-wasm": "1.30.0",
    "kysely": "0.27.6",
    "web-worker": "1.5.0"
  },
  "devDependencies": {
    "@eslint/js": "9.39.0",
    "@rslib/core": "0.18.3",
    "@rstest/core": "0.7.0",
    "@types/node": "24.10.1",
    "@rstest/coverage-istanbul": "0.1.0",
    "eslint": "9.39.0",
    "globals": "16.4.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.46.2"
  }
}
